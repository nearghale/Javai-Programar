<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.matheasy.meuprojeto">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mascote_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mascote_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivitySemPerfil"
            android:label="@string/title_activity__login"
            android:theme="@style/AppTheme" />
        <activity android:name=".AddPerfilActivity" />
        <activity
            android:name=".PerfisActivity"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity_escolherImagem" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".EdtPerfilActivity" />
        <activity android:name=".activity_escolher_opcao" />
        <activity android:name=".Welcome_Activity" />
        <activity android:name=".Activity_escolherDificuldade" />
        <activity android:name=".activity_conteudo_aula" />
        <activity android:name=".Activity_oque_ira_aprender_capitulo"></activity>
    </application>

</manifest>